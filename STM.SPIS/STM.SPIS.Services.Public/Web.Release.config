  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>

  <system.webServer>
    <security>
      <access sslFlags="Ssl,SslNegotiateCert" xdt:Transform="Insert" />
    </security>
  </system.webServer>
</configuration>