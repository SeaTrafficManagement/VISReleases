<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings>
    <add name="StmConnectionString" providerName="System.Data.SqlCLient" connectionString="Data Source=tcp:smastmvistest.database.windows.net,1433; Initial Catalog={database};Persist Security Info=False;User ID=stmtestadmin;Password=ExtraordinaryShipping!;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
      xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
